<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>${firefox.ID}</em:id>
    <em:version>${version}</em:version>
    <em:type>2</em:type>
    <em:iconURL>chrome://ytcenter/content/icon32.png</em:iconURL>
    <em:iconURL64>chrome://ytcenter/content/icon64.png</em:iconURL64>
    <em:optionsURL>http://www.youtube.com/#ytcenter.settings.open</em:optionsURL>
    <em:optionsType>3</em:optionsType>
    <em:bootstrap>true</em:bootstrap>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>19.0</em:minVersion>
        <em:maxVersion>26.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Seasmonkey -->
    <em:targetApplication>
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>2.31</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Firefox Mobile -->
    <em:targetApplication>
      <Description>
        <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
        <em:minVersion>19.0</em:minVersion>
        <em:maxVersion>20.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>${name}</em:name>
    <em:description>${description}</em:description>
    <em:creator>${author}</em:creator>
    
    <!-- Translators -->
  </Description>
</RDF>
